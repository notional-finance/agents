{
  "name": "@notional-finance/agent",
  "version": "0.2.3",
  "description": "Agents for Notional Finance",
  "main": "index.ts",
  "repository": "git@github.com:notional-finance/agents.git",
  "author": "Jeff Wu <jeff@notional.finance>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./src/app.ts",
    "dev": "nodemon",
    "typechain": "typechain --target=ethers-v5 --outDir src/core/typechain './src/core/abi/*.json'",
    "typechain:mocks": "typechain --target=ethers-v5 --outDir test/mocks/ './test/mocks/*.json'",
    "build": "yarn run typechain && yarn run build:config && tsc -b tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "build:config": "yarn run ts-interface-builder src/config/configSchema.ts",
    "build:docker": "yarn run build && docker build -t notional/agents:$(jq  -j '.version' < package.json) -t notional/agents:latest .",
    "publish:docker": "docker push notional/agents:$(jq  -j '.version' < package.json) && docker push notional/agents:latest",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "generate": "graphql-codegen --config graphql-codegen.yml",
    "build:contracts": "contracts/build.sh",
    "test": "jest --selectProjects unit",
    "test:watch": "yarn run jest --selectProjects unit --config=jest.config.js --watch",
    "test:integration": "./start.sh && export RUN_SETUP=true && jest --runInBand --selectProjects integration --config=jest.config.js --detectOpenHandles",
    "test:integration:run": "jest --config=jest.config.js --runInBand --selectProjects integration --detectOpenHandles",
    "deploy:flashswap:kovan": "DOTENV_CONFIG_PATH=.env.kovan yarn run ts-node -r tsconfig-paths/register contracts/deploy.ts",
    "deploy:flashswap:mainnet": "DOTENV_CONFIG_PATH=.env.mainnet yarn run ts-node -r tsconfig-paths/register contracts/deploy.ts"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-apollo-client-helpers": "^1.1.0",
    "@graphql-codegen/typescript-operations": "^1.17.9",
    "@typechain/ethers-v5": "^3.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@uniswap/v2-core": "^1.0.1",
    "@uniswap/v2-periphery": "^1.1.0-beta.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-interface-builder": "^0.2.3",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typechain": "^4.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@awaitjs/express": "^0.6.3",
    "@uniswap/sdk": "jeffywu/uniswap-sdk#e51b6b8",
    "axios": "^0.21.0",
    "axios-retry": "^3.1.9",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.19",
    "express": "^4.17.1",
    "express-jsdoc-swagger": "^1.3.0",
    "graphql": "^15.4.0",
    "js-yaml": "^3.14.0",
    "limiter": "^1.1.5",
    "log4js": "^6.3.0",
    "prom-client": "^12.0.0",
    "prometheus-api-metrics": "^3.1.0",
    "querystring": "^0.2.0",
    "ts-interface-checker": "^0.1.13",
    "tslib": "^2.0.3",
    "url": "^0.11.0"
  }
}
